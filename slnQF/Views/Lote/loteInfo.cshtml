@model slnQF.SP_CONSULTA_SAP_LOTE_INFOResult

    <h4>Informacion de Lote</h4>
     <br />

<div class="form-row">
    <div class="form-group col-md-6">
        <label for="DistNumber">Lote</label>

        @Html.TextBoxFor(model => model.DistNumber, new { @class = "form-control", @readonly = "readonly" })

    </div>
    <div class="form-group col-md-6">
        <label for="ESTADO">Estado</label>

        @Html.TextBoxFor(model => model.ESTADO, new { @class = "form-control", @readonly = "readonly" })

    </div>
</div>
<div class="form-row">
    <div class="form-group col-md-6">
        <label for="MnfSerial">Estatus</label>
        @Html.TextBoxFor(model => model.MnfSerial, new { @class = "form-control", @readonly = "readonly" })
    </div>
    <div class="form-group col-md-6">
        <label for="LotNumber">Potencia:</label>
        @Html.TextBoxFor(model => model.LotNumber, new { @class = "form-control", @readonly = "readonly" })
    </div>
</div>
<div class="form-row">
    <div class="form-group col-md-6">
        <label for="FECHA_ADMIN">Admision:</label>
        @Html.TextBoxFor(model => model.FECHA_ADMIN, new { @class = "form-control", @readonly = "readonly" })
    </div>
    <div class="form-group col-md-6">
        <label for="FECHA_FABRI">Fabricacion</label>
        @Html.TextBoxFor(model => model.FECHA_FABRI, new { @class = "form-control", @readonly = "readonly" })
    </div>
</div>
<div class="form-row">
    <div class="form-group col-md-6">
        <label for="FECHA_VENCIM">Vencimiento:</label>
        @Html.TextBoxFor(model => model.FECHA_VENCIM, new { @class = "form-control", @readonly = "readonly" })
    </div>
    <div class="form-group col-md-6">
        <label for="U_CERTICA">Certifica:</label>
        @Html.TextBoxFor(model => model.U_CERTICA, new { @class = "form-control", @readonly = "readonly" })
    </div>
</div>

<div class="form-row">
    <div class="form-group col-md-12">
        <label for="NOTAS">Notas:</label>
        @Html.TextAreaFor(model => model.NOTAS, new { @class = "form-control", @readonly = "readonly" })
    </div>
    
</div> 